				</e:Group>
				<e:List id="list" top="95" bottom="20" horizontalCenter="0">
					<e:layout>
					<e:TileLayout horizontalGap="10" requestedColumnCount="3" paddingLeft="0"/>
					</e:layout>
				</e:List>
			</e:Group>
			<e:layout>
				<e:VerticalLayout paddingBottom="10" paddingTop="0" gap="60" horizontalAlign="center"/>
