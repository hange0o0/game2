<?xml version="1.0" encoding="utf-8"?>
<e:Skin class="MainFightUISkin" width="640" height="960" xmlns:e="http://ns.egret.com/eui" xmlns:ns1="*" xmlns:w="http://ns.egret.com/wing">

	<e:Scroller id="scroller" width="640" horizontalCenter="0" top="0" bottom="0" bounces="false">
		<e:Group>
			<e:Group x="0" y="65" width="640">
				<ns1:HangUI id="mapBtn" x="0" y="75" skinName="HangUISkin" height="450"/>
				<e:Image source="line_png" y="65" scale9Grid="42,2,258,18" width="660" fillMode="repeat" horizontalCenter="0"/>
				<e:Group id="forceGroup" x="0" y="10">
					<e:Image source="black_bg2_png" x="0" y="0" height="65" width="300"/>
					<e:Image source="icon_force_png" x="12" y="0"/>
					<e:Label text="战力：" x="47" size="30" textColor="0xFCC541" stroke="2" strokeColor="0x662A0E" italic="true" bottom="10"/>
					<e:Label id="forceText" text="12345" x="125" size="42" textColor="0xfcc541" stroke="3" strokeColor="0x662a0e" italic="true" bottom="5"/>
				</e:Group>
				<e:Group y="0" height="75" right="0">
					<e:Group id="shopBtn" x="100" y="0" height="80" width="80">
						<e:Image source="main_shop_png" verticalCenter="0" horizontalCenter="0"/>
						<e:Label text="商城" size="22" horizontalCenter="0" stroke="2" bottom="10"/>
					</e:Group>
					<e:Group id="rankBtn" x="100" y="0" height="80" width="80">
					<e:Image source="main_rank_png" horizontalCenter="0" verticalCenter="0"/>
					<e:Label text="排行" size="22" horizontalCenter="0" stroke="2" bottom="10"/>
					</e:Group>
					<e:Group id="mailBtn" x="10" y="0" width="80" height="80">
						<e:Image source="main_mail_png" verticalCenter="0" horizontalCenter="0"/>
						<e:Label text="消息" size="22" horizontalCenter="0" stroke="2" bottom="10"/>
						<e:Image id="mailRed" source="red_mc_png" right="0" top="12"/>
					</e:Group>
					<e:Group id="settingBtn" x="100" y="0" width="80" height="80">
					<e:Image source="icon_setting_png" horizontalCenter="0" verticalCenter="0"/>
					<e:Label text="设置" size="22" horizontalCenter="0" stroke="2" bottom="10"/>
					</e:Group>
					<e:layout>
					<e:HorizontalLayout horizontalAlign="right" paddingLeft="0" paddingRight="0" gap="6"/>
					</e:layout>
				</e:Group>
			</e:Group>
			<ns1:PKActiveMainPageUI id="activeUI" x="156" y="660" skinName="PKActiveMainPageUISkin" height="450"/>
			<e:layout>
				<e:VerticalLayout gap="0" paddingTop="65" horizontalAlign="center" paddingBottom="0"/>
			</e:layout>
		</e:Group>
	</e:Scroller>
	<e:Group id="defBtn" bottom="0" width="110" scaleX="0.8" scaleY="0.8" scrollEnabled="true" x="0">
		<e:Image source="btn7_png" y="0" right="0"/>
		<e:Image source="icon_def1_png" verticalCenter="-5" horizontalCenter="-10"/>
	</e:Group>
	<e:Group id="atkBtn" bottom="0" width="110" right="0" scaleX="0.8" scaleY="0.8" scrollEnabled="true">
		<e:Image source="btn5_png" x="0" y="0" scaleX="1.53" scaleY="1.53"/>
		<e:Image source="icon_atk3_png" verticalCenter="0" horizontalCenter="10"/>
	</e:Group>
</e:Skin>